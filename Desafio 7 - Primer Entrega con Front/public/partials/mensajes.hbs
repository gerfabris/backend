<div>
    {{#each messages }}
        <p class"my-2 messages__container">
            <span class="text-info email">{{ author }} </span>
            <span class="date"> 
                [{{ fecha.day }}/{{ fecha.month}}/{{ fecha.year }} 
                - 
                {{ fecha.hs }}:{{ fecha.min }}:{{ fecha.sec }}]:    
            </span>
            <span class="text-success">: {{text}} </span>
        </p>
    {{/each}}
</div>
